{
  "name": "@crystallize/react-basket",
  "description": "React component to ease integration with Crystallize",
  "version": "7.0.0",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "url": "git@github.com:CrystallizeAPI/react-basket.git",
    "type": "git"
  },
  "main": "dist/index.js",
  "scripts": {
    "prepush": "yarn validate",
    "dev": "parcel dev/index.html",
    "lint": "eslint ./module",
    "validate": "lint-staged",
    "build": "yarn rollup -c --sourcemap",
    "prepare": "yarn build"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "prettier": {
    "singleQuote": true
  },
  "eslintIgnore": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=16.4.0",
    "react-dom": ">=16.4.0",
    "styled-components": ">=4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@crystallize/react-image": "^2.3.0",
    "body-parser": "^1.18.2",
    "bounce": "^1.2.0",
    "cross-fetch": "^2.2.3",
    "i18next": "^15.1.0",
    "isarray": "^2.0.4",
    "localforage": "^1.6.0",
    "prop-types": "^15.7.2",
    "react-pose": "^4.0.8",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.10.0",
    "dotenv": "^5.0.1",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "graphql": "^14.3.0",
    "helmet": "^3.10.0",
    "husky": "^0.14.3",
    "lint-staged": "^8.1.6",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.14.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.9.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "styled-components": "^4.2.0",
    "styled-is": "^1.3.0",
    "urql": "^1.0.5"
  }
}
